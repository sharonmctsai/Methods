//3.a example 3.8

//drawing six eyes

void setup()
{
 size(100,100);
 noStroke();
}

void draw()
{
 background(204);
 
 //Right eye
 fill(255);       //outer white circle
 circle(65,44,60);
 
 fill(0);        //black circle
 circle(65+10,44,30);
 
 fill(255);      //inner white curcle
 circle(65+16,44-5,6);
 
 //Left eye
 fill(255);       //outer white circle
 circle(20,50,60);
 
 fill(0);        //black circle
 circle(20+10,50,30);
 
 fill(255);      //inner white curcle
 circle(20+16,50-5,6);
}
