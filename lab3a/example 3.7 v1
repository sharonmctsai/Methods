//3.a example 3.7 v1

//drawing a single eye

void setup()
{
 size(100,100);
 noStroke();
}

void draw()
{
 background(204);
 fill(255);       //outer white circle
 circle(50,50,60);
 
 fill(0);        //black circle
 circle(50+10,50,30);
 
 fill(255);      //inner white curcle
 circle(50+16,46,6);
 
  
}
